@{
    ViewBag.Title = "Index";
}
@using StoreMVC.Models

<h2>Shopping Cart</h2>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Buy</th>
    </tr>
    @foreach (Product p in ViewBag.listProducts)
    {
        <tr>
            <td>@p.ID</td>
            <td>@p.Name</td>
            <td>@p.Price</td>
            <td>Order Now</td>
        </tr>
    }
</table>